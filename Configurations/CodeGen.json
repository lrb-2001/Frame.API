{
  "$schema": "https://gitee.com/dotnetchina/Furion/raw/v4/schemas/v4/furion-schema.json",

  // 代码生成配置项-程序集名称集合
  "CodeGen": {
    "EntityAssemblyNames": [ "Frame.Core", "Frame.System.Entity" ],
    "BaseEntityNames": [ "BaseEntity", "PrimaryKey" ],
    "EntityBaseColumn": {
      "BaseEntity": [ "Id", "CreationTime", "ModifierTime", "CreatorUserId", "ModifierUserId", "DeleterUserId", "DeletionTime", "IsDelete" ],
      "PrimaryKey": [ "Id" ]
    },
    "FrontRootPath": "Web", // 前端文件根目录
    "BackendApplicationNamespace": "Frame.System" // 后端生成到的项目
  }
}